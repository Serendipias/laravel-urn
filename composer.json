{
    "name": "serendipias/laravel-urn",
    "description": "Universal laravel urn identifier",
    "keywords": ["urn", "urn-identifier", "composer", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Y. C. Rodriguez",
            "email": "jorge@serendipia.one"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "symfony/intl": "^4.3|^5.0",
        "psy/psysh": "^0.9",
        "league/uri": "^5.3",
        "ramsey/uuid": "^3.8",
        "illuminate/contracts": "^6.6",
        "illuminate/support": "^6.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Serendipias\\Urn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serendipias\\Urn\\Tests\\": "tests/"
        }
    }
}
